{
  "name": "ecs-deploy-cli",
  "version": "4.1.1",
  "packageManager": "yarn@3.6.3",
  "bin": {
    "ecs-deploy": "./dist/sh.js"
  },
  "scripts": {
    "start": "tsx ./src/sh.ts",
    "test": "tsx --test ./src/**/*.test.ts",
    "test:watch": "tsx --watch --test ./src/**/*.test.ts",
    "build:clean": "rm -rf ./dist",
    "build": "yarn build:clean && node ./esbuild.mjs && chmod +x ./dist/sh.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "povio",
    "ecs",
    "ecr",
    "deploy",
    "aws"
  ],
  "homepage": "https://github.com/poviolabs/ecs-deploy-cli",
  "bugs": "https://github.com/poviolabs/ecs-deploy-cli/issues",
  "repository": {
    "url": "https://github.com/poviolabs/ecs-deploy-cli",
    "type": "git"
  },
  "contributors": [
    "Marko Zabreznik <marko.zabreznik@povio.com>"
  ],
  "devDependencies": {
    "@aws-sdk/client-ecr": "3.408.0",
    "@aws-sdk/client-ecs": "3.408.0",
    "@aws-sdk/client-ssm": "3.408.0",
    "@aws-sdk/client-sts": "3.408.0",
    "@aws-sdk/credential-providers": "^3.408.0",
    "@aws-sdk/types": "3.408.0",
    "@smithy/node-config-provider": "^2.0.9",
    "@types/diff": "^5.0.3",
    "@types/node": "^20.5.9",
    "@types/prettier": "^3.0.0",
    "@types/prompt-sync": "^4.2.0",
    "@types/semver": "^7.5.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "chalk": "^5.3.0",
    "cosmiconfig": "^8.3.5",
    "diff": "^5.1.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "js-yaml": "^4.1.0",
    "lodash.merge": "^4.6.2",
    "prettier": "^3.0.3",
    "prompt-sync": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.5.4",
    "simple-git": "^3.19.1",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "yargs": "^17.7.2",
    "zod": "^3.22.2"
  },
  "engines": {
    "node": ">= 14",
    "yarn": ">= 3.2",
    "npm": ">= 8"
  }
}
