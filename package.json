{
  "name": "@poviolabs/ecs-deploy-cli",
  "version": "0.11.15",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "lint": "exlint --fix",
    "test:ts-node": "ts-node -r tsconfig-paths/register ./src/index.ts --pwd ./test --stage test",
    "test": "./dist/index.js --pwd ./test --stage test",
    "build": "yarn version patch && yarn ncc build ./src/index.ts -m --no-source-map-register --stats-out ./dist/stats.json && chmod +x ./dist/index.js",
    "clean": "rm ./dist/*"
  },
  "keywords": [
    "povio",
    "ecs",
    "ecr",
    "deploy",
    "aws"
  ],
  "homepage": "https://github.com/poviolabs/ecs-deploy-cli",
  "bugs": "https://github.com/poviolabs/ecs-deploy-cli/issues",
  "repository": {
    "url": "https://github.com/poviolabs/ecs-deploy-cli",
    "type": "git"
  },
  "contributors": [
    "Marko Zabreznik <marko.zabreznik@poviolabs.com>"
  ],
  "bin": "./dist/index.js",
  "devDependencies": {
    "@aws-sdk/client-ecr": "^3.67.0",
    "@aws-sdk/client-ecs": "^3.67.0",
    "@aws-sdk/credential-provider-env": "^3.55.0",
    "@aws-sdk/credential-provider-ini": "^3.67.0",
    "@aws-sdk/node-config-provider": "^3.58.0",
    "@jest/types": "^27.5.1",
    "@slack/web-api": "^6.7.1",
    "@swc/core": "^1.2.165",
    "@swc/jest": "^0.2.20",
    "@types/diff": "^5.0.2",
    "@types/eslint": "^8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/prettier": "^2.6.0",
    "@types/prompt-sync": "^4.1.1",
    "@types/semver": "^7.3.9",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vercel/ncc": "^0.33.4",
    "chalk": "^4.1.2",
    "diff": "^5.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "prompt-sync": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.7",
    "simple-git": "^3.6.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "4.6.3",
    "yaml": "^2.0.0",
    "yargs": "^17.4.1"
  },
  "engines": {
    "node": ">= 16.14.2",
    "yarn": ">= 3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.67.0",
    "lodash": "^4.17.21",
    "lodash.snakecase": "^4.1.1"
  }
}
